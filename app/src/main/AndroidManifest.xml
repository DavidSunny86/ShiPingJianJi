<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qiniu.pili.droid.shortvideo.demo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.cheerchip.shipingjianji.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cheerchip.shipingjianji.VideoRecordActivity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.GifMakeActvity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.GifShowActivity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.VideoActivity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.VideoTrimActivity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.CameraActivity1"></activity>
        <activity android:name="com.cheerchip.shipingjianji.ScreenRecordActivity"></activity>
        <activity android:name="com.cheerchip.shipingjianji.PlaybackActivity"></activity>
    </application>

</manifest>